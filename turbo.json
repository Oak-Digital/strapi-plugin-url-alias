{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "build:release": {
      "dependsOn": ["^build:release"]
    },
    "eslint": {
      "outputs": []
    },
    "eslint:fix": {
      "outputs": []
    },
    "develop": {
      "cache": false
    }
  }
}
